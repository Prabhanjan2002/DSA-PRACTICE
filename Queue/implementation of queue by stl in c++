#include<iostream>
#include<queue>
using namespace std;

int main()
{
    queue<int>q;
    q.push(1);
    q.push(3);
    q.push(4);
    q.push(5);

    cout<<"front of queue is => "<<q.front()<<endl;   //1
    cout<<"rear of queue is => "<<q.back()<<endl;    //5

    q.pop();

    cout<<"front of queue is => "<<q.front()<<endl;    //3
    cout<<"rear of queue is => "<<q.back()<<endl;    //5
    
    if(q.empty())
    cout<<"queue is empty"<<endl;   //queue is not empty
    else
    cout<<"queue is not empty"<<endl;
    
    return 0;
    }
