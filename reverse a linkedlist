#include<iostream>
using namespace std;
class Node{
    public:
    int data;
    Node* next;

    Node(int data)
    {
        this->data=data;
        this->next=NULL;
    }
};
void insertattail(node* &tail,int d)
{
   node* temp=new node(d);
   tail->next=temp;
   tail=tail->next;
}
void print(node* &head)
{
   node* temp=head;
   while(temp!=NULL)
   {
      cout<<temp->data<<"->";
      temp=temp->next;
   }
   cout<<endl;
}
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    for(int i=0;i<n;i++)
    {
        insertattail(tail,arr[i]);
        cout<<arr[i];
    }
    print(head);
}
